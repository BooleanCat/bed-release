check process broker
  with pidfile /var/vcap/sys/run/broker.pid
  start program /var/vcap/jobs/broker/bin/broker/start.sh
  stop program /var/vcap/jobs/broker/bin/broker/stop.sh

check process nginx
  with pidfile /var/vcap/sys/run/nginx.pid
  start program /var/vcap/jobs/broker/bin/nginx/start.sh
  stop program /var/vcap/jobs/broker/bin/nginx/stop.sh
